{
  "IsEncrypted": false,
  "Values": {
    "AzureWebJobsStorage": "@Microsoft.KeyVault(VaultName=seu-keyvault;SecretName=storage-connection-string)",
    "FUNCTIONS_WORKER_RUNTIME": "dotnet-isolated",
    "AZURE_SUBSCRIPTION_ID": "@Microsoft.KeyVault(VaultName=seu-keyvault;SecretName=azure-subscription-id)",
    "AZURE_TENANT_ID": "@Microsoft.KeyVault(VaultName=seu-keyvault;SecretName=azure-tenant-id)",
    "AZURE_CLIENT_ID": "@Microsoft.KeyVault(VaultName=seu-keyvault;SecretName=azure-client-id)"
  },
  "ConnectionStrings": {
    "CosmosDb": "@Microsoft.KeyVault(VaultName=seu-keyvault;SecretName=cosmos-connection-string)"
  },
  "CosmosDb": {
    "DatabaseName": "SmartCostDB",
    "CostRecordsContainer": "CostRecords",
    "EventsContainer": "Events"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "AzureSmartCost": "Information"
    }
  }
}